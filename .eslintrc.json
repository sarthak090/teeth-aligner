{
  "extends": ["eslint:recommended", "next", "next/core-web-vitals", "prettier"],
  "plugins": ["prettier", "tailwindcss"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
    "@next/next/no-img-element": "off",
    "no-unused-vars": "off",
    "react/no-unescaped-entities": "off",
    "tailwindcss/classnames-order": "warn"
  }
}